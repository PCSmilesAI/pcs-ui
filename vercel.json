{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.{js,cjs}",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhooks/quickbooks",
      "dest": "/api/webhooks/quickbooks.cjs"
    },
    {
      "src": "/api/webhooks/test",
      "dest": "/api/webhooks/test.cjs"
    },
    {
      "src": "/api/qbo/complete-oauth",
      "dest": "/api/qbo/complete-oauth.cjs"
    },
    {
      "src": "/api/qbo/debug-env",
      "dest": "/api/qbo/debug-env.cjs"
    },
    {
      "src": "/api/qbo/test-simple",
      "dest": "/api/qbo/test-simple.cjs"
    },
    {
      "src": "/api/qbo/sync-categories",
      "dest": "/api/qbo/sync-categories.cjs"
    },
    {
      "src": "/api/qbo/send-invoice",
      "dest": "/api/qbo/send-invoice.cjs"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/invoice_queue.json",
      "dest": "/invoice_queue.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/office_info.json",
      "dest": "/office_info.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/email_invoices/(.*)",
      "dest": "/email_invoices/$1"
    },
    {
      "src": "/output_jsons/(.*)",
      "dest": "/output_jsons/$1"
    },
    {
      "src": "/test-oauth-completion.html",
      "dest": "/public/test-oauth-completion.html"
    },
    {
      "src": "/quickbooks-integration-test.html",
      "dest": "/public/quickbooks-integration-test.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 