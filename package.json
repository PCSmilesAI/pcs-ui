{
  "name": "pcs-ui",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "dev:api": "node dev-server.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node dev-server.js",
    "start:prod": "NODE_ENV=production node dev-server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "backup": "node -e \"require('./dev-server.js').pcsAIDatabase.backupData()\"",
    "health": "curl -s http://localhost:3001/health | jq",
    "metrics": "curl -s http://localhost:3001/metrics | jq"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "intuit-oauth": "^4.0.0",
    "node-quickbooks": "^2.0.46",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "pm2": "^6.0.8",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
